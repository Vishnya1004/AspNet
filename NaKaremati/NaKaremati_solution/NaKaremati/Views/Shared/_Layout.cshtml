<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="main-pannel">
        <ul class="main-pannel">
            <li>@Html.ActionLink("NaKaremati", "Index", "Home", new { area = "" }, null)</li>
            <li>@Html.ActionLink("How beautiful we are", "About", "Home", new { area = "" }, null)</li>
            <li>@Html.ActionLink("Gallery", "Gallery", "Home", new { area = "" }, null)</li>
            <li>@Html.ActionLink("Useful articles", "Articles", "Home", new { area = "" }, null)</li>
            <li>@Html.ActionLink("Our lovely partners", "Partners", "Home", new { area = "" }, null)</li>
            <li>@Html.ActionLink("Be our partners", "BePartners", "Home", new { area = "" }, null)</li>
            @Html.Partial("_LoginPartial")
        </ul>
       
       
    </div> 
  
    
    

    @RenderBody()
  
    <footer>
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
